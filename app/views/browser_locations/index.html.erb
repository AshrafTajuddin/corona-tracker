<p id="notice"><%= notice %></p>

<h1>Browser Locations</h1>

<table>
  <thead>
    <tr>
      <th>Lonlat</th>
      <th>Accuracy</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @browser_locations.each do |browser_location| %>
      <tr>
        <td><%= browser_location.lonlat %></td>
        <td><%= browser_location.accuracy %></td>
        <td><%= link_to 'Show', browser_location %></td>
        <td><%= link_to 'Edit', edit_browser_location_path(browser_location) %></td>
        <td><%= link_to 'Destroy', browser_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Browser Location', new_browser_location_path %>
